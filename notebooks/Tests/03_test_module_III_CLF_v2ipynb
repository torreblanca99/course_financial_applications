      "cell_type": "code",
      "source": [
        "model_nn = keras.models.Sequential([\n",
        "  keras.layers.Input(shape=(X_train_transfomed.shape[1],), name=\"input\"),\n",
        "  #keras.layers.Dense(900, name='first_layer', activation=\"linear\"),\n",
        "  keras.layers.Dense(400, name='third_layer', activation=\"linear\"),\n",
        "  keras.layers.Dense(100, name='fifth_layer', activation=\"relu\"),\n",
        "  keras.layers.Dense(10, name='output', activation='softmax')\n",
        "])\n",
        "\n",
        "model_nn.compile(\n",
        "    loss='sparse_categorical_crossentropy',\n",
        "    optimizer=keras.optimizers.Adam(learning_rate=1e-3),\n",
        "    metrics=['accuracy']\n",
        ")\n",
        "\n",
        "\n",
        "model_nn.summary()"
